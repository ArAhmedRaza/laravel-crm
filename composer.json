{
    "name": "venturedrake/laravel-crm-install",
    "type": "project",
    "description": "Install and setup a complete Laravel CRM application",
    "keywords": [
        "venturedrake",
        "laravel",
        "free crm",
        "open source crm",
        "CRM",
        "laravel-crm",
        "laravel crm",
        "laravel crm package",
        "contact management",
        "contact management system",
        "customer management",
        "customer relationship management",
        "lead management",
        "sales management",
        "crm system",
        "crm online",
        "crm platform"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.0",
        "rachidlaasri/laravel-installer": "^4.1",
        "venturedrake/laravel-crm": "^0.1.4"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^2.18",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nascent-africa/jetstrap": "^2.2",
        "nunomaduro/collision": "^5.0",
        "phpstan/phpstan": "^0.12.82",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=no",
        "analyse": "vendor/bin/phpstan analyse -c phpstan.neon"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
